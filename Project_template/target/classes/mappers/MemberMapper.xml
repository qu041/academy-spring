<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<!-- XML Mapper -->
<mapper namespace="com.test.project.mapper.MemberMapper">

	<select id="get" parameterType="String" resultType="udto">
		
		<!-- username, password enabled  -->
		select 
			id,
			name,
			pw,
			email,
			age,
			regdate,
			auth
			from tblUser where id = #{username} 	
	</select>
	
	<insert id="add" parameterType="udto">
		insert into tblUser(id,name,pw,email,age,regdate,auth) values (#{id},#{name},#{pw},#{email},#{age},default,'ROLE_MEMBER')
	</insert>

	
	
	
</mapper>
















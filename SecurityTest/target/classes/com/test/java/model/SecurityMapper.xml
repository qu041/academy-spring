<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<!-- XML Mapper -->
<mapper namespace="com.test.java.model.SecurityMapper">

	<select id="time" resultType="String">
	select sysdate from dual
	</select>
	
	<insert id="add" parameterType="com.test.java.model.MemberDTO">
		insert into member (memberid,memberpw,membername,email,gender,enabled,regdate) values (#{memberid},#{memberpw},#{membername},#{email},#{gender},default,default)
	</insert>

	
	
	
</mapper>















